#include <stdio.h>

int fibonacci(int n)
{
	if (n == 1)
		return 1;
	if (n == 2)
		return 1;
	else
		return fibonacci(n - 1) + fibonacci(n - 2);

}

int main()
{
	int n, fib;
	printf("Koji clan Fibonaccijevog niza zelite ispisati?\n");
	scanf("%d", &n);
	fib = fibonacci(n);
	printf("%d. clan Fibonaccijevog niza je %d", n, fib);
	return 0;
}
